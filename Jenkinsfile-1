pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
    stage('Build image') {
      steps{
        script {
          docker.build "test9877/java-web-app:latest" 
      }
    }
}
    stage('Scan') {
      steps {
        sh 'trivy test9877/java-web-app:latest'
      }
    }
  }

